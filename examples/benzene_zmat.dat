# Benzene HF/6-311G no dummy atoms

initial_guess = SAD

# DIIS extrapolation steps
diis_range = 8

# Basis set
basis_set = 6-311G

# turn on symmetry
use_symmetry = true

# force point group
#point_group_override = D2h

# frequencies
#frequencies = SCF

# Units used by Z matrix
units = angstrom
{
	C
	C 1 1.380
	C 2 1.380 1 120.000
	C 3 1.380 2 120.000 1   0.000
	C 4 1.380 3 120.000 2   0.000
	C 5 1.380 4 120.000 3   0.000
	H 1 1.085 2 120.000 3 180.000
	H 2 1.085 1 120.000 6 180.000
	H 3 1.085 2 120.000 1 180.000
	H 4 1.085 3 120.000 2 180.000
	H 5 1.085 4 120.000 3 180.000
	H 6 1.085 5 120.000 4 180.000
}

